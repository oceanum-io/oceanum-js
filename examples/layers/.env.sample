# Copy this file to .env and fill in the values

# Datamesh token for authenticating with the zarr service
VITE_DATAMESH_TOKEN=

# Mapbox access token (optional — the map background is hidden without it)
VITE_MAPBOX_TOKEN=

# Mapbox style URL (optional — defaults to mapbox://styles/mapbox/dark-v11)
# VITE_MAPBOX_STYLE=mapbox://styles/mapbox/dark-v11
